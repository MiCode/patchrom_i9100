					<Element name="Weekly" type="RECURRENCE_SUPPORT_FIELD_DOMESTICNONE" />
					<Element name="Fortnightly" type="RECURRENCE_SUPPORT_FIELD_DOMESTICNONE" />
					<Element name="MonthlyNth" type="RECURRENCE_SUPPORT_FIELD_DOMESTICNONE" />
					<Element name="Monthly" type="RECURRENCE_SUPPORT_FIELD_DOMESTICNONE" />
					<Element name="Yearly" type="RECURRENCE_SUPPORT_FIELD_DOMESTICNONE" />
				</Enum>
				<Enum name="RECURRENCE_TYPE_ANNIVERSARY_A">
					<Element name="None" type="bool" />
