			</Enums>
			<Appointment>
				<DataField>
					<Title type="String" charset="utf-16" maxLength="2000" />
					<StartDate type="DateTime" />
					<EndDate type="DateTime" />
					<AllDay type="Boolean" />
					<!-- sh_zip.kim S1(I9000)의 경우 Calendar account 설정 하는 부분이 있습니다. 어떤 방식으로 
						처리하면 될 지 확인 필요합니다. -->
					<Location type="String" charset="utf-16" maxLength="2000" />
					<Alarm>
						<AlarmTime type="ALARM_TIME_TYPE" />
						<Value type="int" />
					</Alarm>							<!-- sh_zip.kim S1(I9000)의 경우 Customize 항목도 있어서 ALARM_TIME_TYPE 설정도 가능해야 
						할 것으로 보입니다. -->
					<Recurrence type="RECURRENCE_TYPE_APPOINTMENT_ANDROID" />
					<Content type="String" charset="utf-16" maxLength="2000" />
				</DataField>
			</Appointment>

			<!-- sh_zip.kim Anniversary is not used on S1(I9000) <Anniversary> <Title 
				type="String" charset="Unicode" maxLength="30" /> <Content type="String" 
				charset="Unicode" maxLength="100" /> <StartDate type="Date" /> <Alarm> <AlarmTime 
				type="ALARM_TIME_TYPE_B" /> <Time type="Time" /> </Alarm> <Recurrence type="RECURRENCE_TYPE_ANNIVERSARY_B" 
				/> </Anniversary> -->
		</Schedule>

